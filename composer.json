{
    "name": "pdmfc/short-url-package-php-8-support",
    "description": "Url Shortener",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel GonÃ§alves",
            "email": "miguel.goncalves@pdmfc.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pdmfc\\Shorturl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdmfc\\Shorturl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pdmfc\\Shorturl\\Providers\\ShortUrlProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}