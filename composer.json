{
    "name": "pdmfc/shorturl",
    "description": "Url Shortener",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Gon√ßalves",
            "email": "miguel.goncalves@pdmfc.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5|~6.0|~7.0",
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pdmfc\\Shorturl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdmfc\\Shorturl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pdmfc\\Shorturl\\Providers\\ShortUrlProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
